{
  "name": "SortedAI MeetNext",
  "version": "1.0",
  "description": "simple productivity chrome extension, provides a landing page with your next meetings and an option to join them",
  "key": "",
  "oauth2": {
    "client_id": "630020724432-mmh234vkn0flubhv66gfa8op9p547bnv.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/calendar.events.readonly"]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'",
  "permissions": ["tabs", "identity", "storage"],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["js/meetings.js"]
    }],

  "browser_action": {
    "default_title": "SortedAI MeetNext",
    "default_icon": {
      "16": "images/sorted_16.png",
      "32": "images/sorted_32.png",
      "48": "images/sorted_48.png",
      "128": "images/sorted_128.png"
    }
  },
  "icons": {
    "16": "images/sorted_16.png",
    "32": "images/sorted_32.png",
    "48": "images/sorted_48.png",
    "128": "images/sorted_128.png"
  },
  "manifest_version": 2
}
